{
  "name": "wine-menu",
  "version": "1.0.0",
  "description": "Wine Menu",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env dev --progress --profile --colors",
    "build:prod": "webpack --env prod --progress --profile --colors",
    "fix": "prettier --write 'src/**/*.{ts,tsx}' & tslint --fix 'src/**/*.{ts,tsx}'",
    "start": "node server.js & webpack-dev-server --inline --hot --no-info --env dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yumaeda/wine-menu.git"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  },
  "keywords": [
    "react"
  ],
  "author": "Yukitaka Maeda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yumaeda/wine-menu/issues"
  },
  "homepage": "https://github.com/yumaeda/wine-menu#readme",
  "dependencies": {
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "devDependencies": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.2",
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "json-server": "^0.14.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.21.0",
    "ts-loader": "^5.3.1",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react-hooks": "^1.1.0",
    "tslint-webpack-plugin": "^1.3.0",
    "typescript": "^3.5.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.5.0"
  }
}
